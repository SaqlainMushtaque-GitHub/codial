<% if(locals.user) { %>
<div id="user-chat-box">
  <ul id="chat-message-list">
    <li class="sender"></li>
    <li class="my-message"></li>
  </ul>

  <div class="form-chat-box">
    <input id="chat-message" placeholder="Type message here..." />
    <button id="send-message">send</button>
  </div>
</div>
<% } %>
